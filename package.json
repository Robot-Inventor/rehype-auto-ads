{
  "name": "remark-auto-ads",
  "version": "1.0.1",
  "description": "Remark.js plugin that automatically inserts Google Adsense (and theoretically any ad service) code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "format": "prettier ./src/ --write",
    "format:check": "prettier ./src/ --check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robot-Inventor/remark-auto-ads.git"
  },
  "keywords": [
    "remark-plugin",
    "markdown"
  ],
  "author": "Robot-Inventor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Robot-Inventor/remark-auto-ads/issues"
  },
  "homepage": "https://github.com/Robot-Inventor/remark-auto-ads#readme",
  "devDependencies": {
    "@types/mdast": "^4.0.3",
    "prettier": "^3.2.5",
    "remark": "^15.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "unist-util-visit-parents": "^6.0.1"
  }
}
