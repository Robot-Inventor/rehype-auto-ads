{
  "name": "rehype-auto-ads",
  "version": "1.2.1",
  "description": "rehype.js plugin that automatically inserts Google Adsense (and theoretically any ad service) code",
  "publishConfig": {
    "provenance": true
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc",
    "version": "npm run build && git add .",
    "test": "vitest",
    "format": "prettier ./src/ --write",
    "format:check": "prettier ./src/ --check",
    "lint": "eslint ./src/**/*.ts",
    "changeset-version": "changeset version && npm run version",
    "changeset-publish": "npm run build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robot-Inventor/rehype-auto-ads.git"
  },
  "keywords": [
    "rehype-plugin",
    "markdown"
  ],
  "author": "Robot-Inventor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Robot-Inventor/rehype-auto-ads/issues"
  },
  "homepage": "https://github.com/Robot-Inventor/rehype-auto-ads#readme",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@robot-inventor/eslint-config": "^7.0.0",
    "@robot-inventor/tsconfig-base": "^5.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.2.5",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@types/hast": "^3.0.0",
    "hast-util-from-html": "^2.0.0",
    "hast-util-is-element": "^3.0.0",
    "unified": "^11.0.0",
    "unist-util-visit-parents": "^6.0.0",
    "vfile": "^6.0.1"
  }
}
